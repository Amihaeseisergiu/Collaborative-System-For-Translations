{
  "name": "csft",
  "main": "index.js",
  "scripts": {
    "watch": "webpack --watch -d --output ./target/classes/static/built/bundle.js",
    "build": "npm run build:css",
    "build:css": "postcss src/main/resources/static/css/*.css --base src/main/resources/static/css --dir target/classes/static/css",
    "dev": "concurrently \"npm:watch-css\"",
    "watch-css": "onchange -i src/main/resources/static/css/ -- npm run build:css"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.3.0",
    "sass-loader": "^10.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "@fullhuman/postcss-purgecss": "^1.3.0"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "lodash": "^4.17.20",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-form": "^8.3.6",
    "redux-thunk": "^2.3.0",
    "autoprefixer": "^9.6.1",
    "concurrently": "^4.1.2",
    "cssnano": "^4.1.10",
    "onchange": "^6.1.0",
    "postcss-cli": "^6.1.3",
    "tailwindcss": "^1.1.2"
  }
}
